# Coinbase Trading Bot Configuration - PRODUCTION
# Copy this file to .env.production and fill in your production API credentials
# Never commit .env.production to version control!

# Coinbase Production API Credentials
COINBASE_API_KEY=your_production_api_key
COINBASE_API_SECRET=your_production_secret
COINBASE_API_PASSPHRASE=your_production_passphrase

# Trading Configuration (optional - defaults are used if not set)
TRADING_SYMBOL=ETH/USD
TRADING_TIMEFRAME=5m
TRADING_LEVERAGE=5
TRADING_RISK_PCT=0.20
TRADING_ATR_MULTIPLIER=1.5
